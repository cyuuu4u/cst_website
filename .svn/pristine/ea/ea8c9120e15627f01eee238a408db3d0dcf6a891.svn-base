/*英文二级界面*/
/*News界面*/
.row-second{
    position: relative;
    width: 980px;
    height: auto;
    border-bottom: 1px dotted #c9c9c9;
    padding: 28px 0;
}
/*第一级标题*/
.title-one{
    color: black;
    font-family: Helvetica;
    font-size: 24px;
    font-weight: bolder;
    letter-spacing: 6px;
    line-height: 30px;
}
.left_container{
    width:750px;
    height: auto;
    position: relative;
    margin: 0;
    padding: 0;
    float: left;

}
.left_content{
    width: 750px;
    height: auto;
    padding: 5px 0px;
    margin: 10px 0px;
    border-bottom: 1px dotted #c9c9c9;
}
.release_time_en{
    font-family: Helvetica;
    color: #b0b5b5;
    margin:0 0 10px 0;
    font-size: 14px;
    font-weight: bold;
}
.news-image {
    float: right;
    margin: 10px 0 10px 10px;
    width: 195px;
    height: 195px;
    overflow: hidden;
}
.headline{
    color: #7d7d7d;
    font-size: 16px;
    font-weight: bold;
    letter-spacing: 3px;
    line-height: 25px;
    text-align: left;
    text-transform: uppercase;
}
.description{
    color: #b0b5b5;
    font-weight: normal;
    font-size: 12px;
    margin-top: 10px;
    margin-bottom: 18px;
    letter-spacing: 2px;
    line-height: 20px;
    text-align: left;
}
.right-container{
    margin: 20px 10px;
    /*padding: 5px 20px;*/
    float: right;
    position: relative;
    /*border-left: 1px dotted #c9c9c9;*/
}
.right-content{
    position: relative;
}
.title-three{
    /*clear: left;*/
    font-size: 14px;
    font-weight: bold;
    color: #000;
    letter-spacing: 2px;
    line-height: 20px;
    padding: 20px 0 10px 0;
}

/*英文教师介绍页面*/
/*英文二级页面标题*/
.row-second_en{
    position: relative;
    width: 980px;
    height: auto;
    padding: 50px 0 50px 0;
    border-bottom: 1px dotted #b0b5b5;
}

/*英文正文块*/
.content_en{
    float: left;
    position: relative;
    left:0;
    top:0;
    width: 980px;
    height: auto;
}
/*introduction正文*/
p.college-introdution_en{
    font-size: 14px;
    color: #000;
    letter-spacing: 3px;
    line-height: 20px;
    text-align: justify;
}

.left-content_en{
    float: left;
    width: 340px;
    height:380px;
    margin: 29px 35px;
    font-size: 18px;
    font-weight: normal;
    color: #FFFFFF;
    letter-spacing: 3px;
    line-height: 35px;
}

.blue-left-content{
    background-color: #008fd9;
}

.content-top_en{
    width: 980px;
    height: auto;
    margin-top: 50px;
    margin-bottom: 50px;
}

.content-first_en{
    font-size: 14px;
    /*font-weight: bold;*/
    color: #000;
    letter-spacing: 2px;
    line-height: 20px;
    margin-top: 20px;
    margin-bottom: 20px;
    text-align: left;
}
/*第一块的第二段*/
.content-second_en{
    font-size: 12px;
    color: #000;
    letter-spacing: 3px;
    line-height: 20px;
}

.staff_info_en{
    width: 235px;
    height:155px;
    float: left;
    text-align: center;
    padding: 25px;
    margin: 18px;
    border: 1px solid #7d7d7d;
}
.staff_img_en{
    width: 82px;
    height: 82px;
    border-radius: 50%;
    overflow: hidden;
}
.staff_content_en{
    font-family: Helvetica;
    font-size: 14px;
    color: #7d7d7d;
}
.faculty_img{
    width: 371px;
    height: 323px;
    float: left;
}
.teacherimg_en{
    height:170px;
    max-width: 170px;
    margin: 76px;
}