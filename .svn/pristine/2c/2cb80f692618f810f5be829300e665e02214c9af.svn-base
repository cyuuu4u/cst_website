/********************************************************************/

.sub-area {
    width:-webkit-calc(100% - 12px);
    width:-moz-calc(100% - 12px);
    width:calc(100% - 12px);
    height:-webkit-calc(100% - 12px);
    height:-moz-calc(100% - 12px);
    height:calc(100% - 12px);
    padding: 5px;
}

.sub-area_dialog {
    width:-webkit-calc(100% - 22px);
    width:-moz-calc(100% - 22px);
    width:calc(100% - 22px);
    height:-webkit-calc(100% - 22px);
    height:-moz-calc(100% - 22px);
    height:calc(100% - 12px);
    padding: 10px;
}

.panel-footer {
    width:-webkit-calc(100% - 22px);
    width:-moz-calc(100% - 22px);
    width:calc(100% - 22px);
    height:-webkit-calc(100% - 22px);
    height:-moz-calc(100% - 22px);
    height:calc(100% - 22px);
    padding: 10px;
    text-align: center;
}

.panel-header-high {
    height: 20px;
}

form {
    padding: 12px 30px;
}

form p{
    margin: 18px 0;
}

form p > span:first-child{
    display: inline-block;
    width: 80px;
}

form p input[type="text"]{
    width: 240px;
}

form p input[type="file"]{
    width: 240px;
}

form p textarea{
    width: 240px;
    height: 100px;
}

.common-textarea{
    width: 240px;
    height: 100px;
}

.double-list {
    width: 100%;
}

.double-list-with-border {
    width:-webkit-calc(100% - 12px);
    width:-moz-calc(100% - 12px);
    width:calc(100% - 12px);
    min-height: 200px;
    padding: 5px;
    border: ridge 1px #e0ecff;
}

.left-side {
    position: relative;
    float: left;
    left: 0;
    top: 0;
    width:43%;
    min-height:200px;
}

.middle-op {
    position: relative;
    float: left;
    width:14%;
    height:200px;
    text-align: center;
    vertical-align: middle;
}

.middle-op p {
    margin: 20px 0;
}

.right-side {
    position: relative;
    float: right;
    right: 0;
    top: 0;
    width:43%;
    min-height:200px;
}

.date-separator {
    display: inline-block;
    vertical-align: middle;
    width: 16px;
    height: 16px;
    background:url('../icons/icon-from-to.gif') no-repeat center center;
}

.tool-separator {
    display: inline-block;
    width: 0;
    height:100%;
    border-left: 1px solid #ccc;
    border-right: 1px solid #fff;
    vertical-align: middle;
    margin: 1px 10px;
}

.tool-blank {
    display: inline-block;
    width: 0;
    height:100%;
    vertical-align: middle;
    margin: 1px 10px;
}

.datagrid-tool-separator {
    display: inline-block;
    width: 0;
    height:24px;
    border-left: 1px solid #ccc;
    border-right: 1px solid #fff;
    vertical-align: middle;
    margin: 1px 2px;
}

input[type="radio"][type="checkbox"] {
    position: relative;
    vertical-align: middle;
    white-space: nowrap;
    margin: 0;
    padding: 0;
    -moz-border-radius: 5px 5px 5px 5px;
    -webkit-border-radius: 5px 5px 5px 5px;
    border-radius: 5px 5px 5px 5px;
    display:block;
}