@charset "utf-8";

* {
    font-family: "Microsoft YaHei UI", "Microsoft YaHei", 微软雅黑, 黑体, 宋体, sans-serif;
    -webkit-font-smoothing: antialiased
}

html {
    width: 100%;
    height: 100%;
    font-size: 62.5%;
}

body {
    width: 100%;
    height: 100%;
}

body,div,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,input,textarea,p {
    margin: 0;
    padding: 0;
}

ul {
    list-style: none;
}

img,a img {
    border: none;
    vertical-align: middle;
}

a {
    text-decoration: none;
}

a:hover {
    text-decoration: none;
    color: red;
}

/* 清除浮动 */
.clearfix:before, .clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

.alert {
    color: red;
}

span.alert input {
    color: red;
}

